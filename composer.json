{
  "name": "blueways/bw-focuspoint-images",
  "type": "typo3-cms-extension",
  "description": "Image Editor for adding focus points to images",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require-dev": {
    "bk2k/bootstrap-package": "^15.0",
    "friendsofphp/php-cs-fixer": "^3.6.0",
    "saschaegerer/phpstan-typo3": "^1.10",
    "ssch/typo3-rector": "^1.2",
    "typo3/cms-base-distribution": "*",
    "typo3/cms-lowlevel": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Blueways\\BwFocuspointImages\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "web-dir": "public",
      "extension-key": "bw_focuspoint_images"
    }
  },
  "authors": [
    {
      "name": "Maik Schneider",
      "role": "Developer",
      "email": "schneider.maik@me.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "typo3/cms-core": "^12.4 || ^13.4"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "scripts": {
    "php:fixer": "php vendor/bin/php-cs-fixer --config=php-cs-fixer.php fix"
  }
}
